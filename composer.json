{
    "name": "mikailfaruqali/databales",
    "description": "A Laravel package to build server-side DataTables with custom column builders and easy frontend integration.",
    "keywords": [
        "laravel",
        "databales",
        "database",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/databales",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22 || ^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\DataTable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\DataTable\\DataTableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
