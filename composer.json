{
    "name": "mikailfaruqali/datatable",
    "description": "A powerful Laravel package for building server-side DataTables with custom column builders, easy frontend integration, and comprehensive export functionality.",
    "keywords": [
        "laravel",
        "datatable",
        "database",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/datatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0",
        "maatwebsite/excel": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22 || ^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\DataTable\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\DataTable\\DataTableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
